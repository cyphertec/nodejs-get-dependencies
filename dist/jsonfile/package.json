    "react-native-xml2js": "^1.0.3",
    "rn-progress-loader": "^1.0.8",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/runtime": "^7.8.7",
    "@types/jest": "^24.0.11",
    "@types/moment-timezone": "^0.5.12",
